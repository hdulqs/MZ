<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mz.util.dao.UniqueRecordDao">

    <insert id="add" parameterType="com.mz.util.UniqueRecord">
      insert into unique_record(id,remark,created) values(#{id},#{remark},#{created})
    </insert>
</mapper>
